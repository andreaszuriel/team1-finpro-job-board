{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "serve": "turbo run serve",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "postprepare": "bash -c 'chmod ug+x .husky/*'"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@types/express": "^5.0.2",
    "@types/pdfkit": "^0.13.9",
    "eslint": "^8.48.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prisma-erd-generator": "^2.0.4",
    "tsc-alias": "^1.8.10",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "name": "finpro-nextjs-express-prisma",
  "packageManager": "npm@8.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/axios": "^0.14.0",
    "axios": "^1.7.2",
    "midtrans-client": "^1.4.3",
    "pdfkit": "^0.17.1",
    "swr": "^2.3.3",
    "tsx": "^4.19.4"
  }
}
